{
	"name": "todo",
	"version": "1.0.0",
	"description": "todo microfrontend",
	"scripts": {
		"start": "npm run serve",
		"serve": "rimraf dist && webpack serve --config webpack/server.config.js",
		"build": "rimraf dist && yarn tsc && webpack --config webpack/build.config.js",
		"tsc": "tsc -p tsconfig.json --noEmit",
		"test": "vitest",
		"test:coverage": "yarn test --coverage",
		"lint": "eslint . --ext .ts --fix",
		"preview": "http-server ./dist -p 4171"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@plumejs/core": "^4.1.3",
		"@plumejs/testing": "^4.1.1",
		"http-server": "^14.1.1",
		"serve": "^14.2.1"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"copy-webpack-plugin": "^12.0.2",
		"css-loader": "^6.10.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"html-loader": "^5.0.0",
		"html-webpack-plugin": "^5.6.0",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.5",
		"sass": "^1.70.0",
		"sass-loader": "^14.1.0",
		"style-loader": "^3.3.4",
		"ts-loader": "^9.5.1",
		"typescript": "^5.3.3",
		"url-loader": "^4.1.1",
		"vitest": "^1.2.2",
		"webpack": "^5.90.1",
		"webpack-bundle-analyzer": "^4.10.1",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.0.0",
		"webpack-merge": "^5.10.0"
	}
}
